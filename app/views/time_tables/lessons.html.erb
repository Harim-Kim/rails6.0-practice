
<table class="table">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Start Time</th>
    <th scope="col">End Time</th>
    <th scope="col">lesson type</th>
    <th scope="col">enrolled tutor</th>
    <th scope="col">enrolled students</th>
    <th scope="col">enroll</th>
<!--    <th scope="col">unenroll</th>-->
  </tr>
  </thead>
  <tbody>
  <% @lessons.lessons.each do |l| %>
    <tr>
      <th scope="row"><%= l.id %></th>
      <td><%= l.time_table.start_time %></td>
      <td><%= l.time_table.end_time %></td>
      <td><%= l.lesson_type %></td>
      <td><%= l.tutor.email %></td>
      <td><%= l.students.count %></td>
      <td><a href="/student/<%= current_student.id %>/lessons/<%= l.id %>">등록하기</a></td>

    </tr>

  <% end %>


  </tbody>
</table>