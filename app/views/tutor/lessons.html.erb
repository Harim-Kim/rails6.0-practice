<h2>Requested Tutor: <%= @tutor.email %></h2>
<table class="table">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Start Time</th>
    <th scope="col">End Time</th>
    <th scope="col">lesson type</th>
    <th scope="col">enrolled student</th>
    <th scope="col">delete</th>
  </tr>
  </thead>
  <tbody>
  <% @tutor_lessons.each do |l| %>
    <tr>
      <th scope="row"><%= l.id %></th>
      <td><%= l.time_table.start_time %></td>
      <td><%= l.time_table.end_time %></td>
      <td><%= l.lesson_type %></td>
      <%if l.student_id != 0 %>
        <td><%= Student.find(l.student_id).email %></td>
      <% else %>
        <td>x</td>
      <% end %>
      <td><%= link_to 'Destroy', l, method: :delete, data: { confirm: 'Are you sure?' } %></td>

    </tr>

  <% end %>


  </tbody>
</table>